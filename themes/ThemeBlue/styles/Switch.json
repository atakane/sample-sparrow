{
  ".switch": {
    "width": 200,
    "touchEnabled": true,
    "toggle": false,
    "thumbOnColor": "#3F4AB1",
    "thumbOffColor": "#0e1025",
    "toggleOffColor": "#010101",
    "toggleOnColor": "#CCCCCC",
    "visible": true,
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "font": {},
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "minHeight": 31,
    "minWidth": 51,
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    }
  }
}
